# Application Settings
APP_NAME=Analytic Agent
APP_VERSION=1.0.0
ENVIRONMENT=production
DEBUG=false

# Database Settings
DATABASE_URL=postgresql://analytic_user:analytic_password@db:5432/analytic_agent
DATABASE_ECHO=false

# OpenAI Settings
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=4000

# CrewAI Settings
CREWAI_VERBOSE=false
CREWAI_MEMORY=true
CREWAI_MAX_ITERATIONS=3

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Security
SECRET_KEY=your_secret_key_here_change_this_in_production
ACCESS_TOKEN_EXPIRE_MINUTES=30

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads

# Redis
REDIS_URL=redis://redis:6379/0

# LibreChat Frontend Settings
JWT_SECRET=your_jwt_secret_here_change_this_in_production
JWT_REFRESH_SECRET=your_refresh_secret_here_change_this_in_production
API_KEY=your_api_key_here

# MongoDB Settings (for LibreChat)
MONGO_URI=mongodb://mongo:27017/librechat
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=password
MONGO_DATABASE=librechat

# LibreChat Features
ALLOW_REGISTRATION=true
ALLOW_SOCIAL_LOGIN=false
ALLOW_ANONYMOUS_CHAT=false
ENABLE_ANALYTICS=true
ENABLE_FILE_UPLOAD=true
ENABLE_WEBSOCKET=true
CUSTOM_ENDPOINTS=true

# External APIs (optional)
ANALYTICS_API_KEY=your_analytics_api_key
DATA_SOURCE_API_KEY=your_data_source_api_key 